r\n  background: radial-gradient(\r\n    circle,\r\n    rgba(235, 235, 255, 1) 49%,\r\n    rgba(110, 110, 255, 1) 100%\r\n  );\r\n  -webkit-background-clip: text;\r\n  -webkit-text-fill-color: transparent;\r\n  font-size: 32px;\r\n}\r\n\r\n.header__nav {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.header__auth-links {\r\n  display: flex;\r\n  flex-direction: row;\r\n  gap: 30px;\r\n  align-items: center;\r\n}\r\n\r\n.header__auth-login {\r\n  font-family: \"Roboto Condensed\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  font-size: 18px;\r\n  color: white;\r\n  text-decoration: none;\r\n  transition: 0.5s ease-in-out color;\r\n}\r\n\r\n.header__auth-login:hover {\r\n  color: rgb(142, 142, 255);\r\n}\r\n\r\n.header__auth-profile {\r\n  font-family: \"Roboto Condensed\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  font-size: 18px;\r\n  color: white;\r\n  text-decoration: none;\r\n  transition: 0.5s ease-in-out color;\r\n}\r\n\r\n.header__auth-profile:hover {\r\n  color: rgb(142, 142, 255);\r\n}\r\n\r\n.header__auth-logout {\r\n  padding: 6px 7px;\r\n  font-family: \"Roboto Condensed\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  font-size: 18px;\r\n  color: white;\r\n  background-color: rgb(12, 12, 21);\r\n  box-shadow: none;\r\n  border: 0;\r\n  border-radius: 10px;\r\n}\r\n\r\n.upload {\r\n  padding-top: 70px;\r\n  display: flex;\r\n}\r\n\r\n.upload__container {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n.upload__container-inside {\r\n  width: 700px;\r\n  height: 200px;\r\n  background-color: rgb(12, 12, 21);\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  flex-direction: column;\r\n  border-radius: 10px;\r\n}\r\n\r\n.upload__content {\r\n  font-family: \"Montserrat\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  background-color: rgb(21, 21, 37);\r\n  width: 500px;\r\n  /* height: 130px; */\r\n  padding-bottom: 90px;\r\n  padding-left: 30px;\r\n  padding-top: 20px;\r\n  border: 0;\r\n  border-radius: 10px;\r\n  margin-bottom: 20px;\r\n  color: white;\r\n}\r\n\r\n.upload__send {\r\n  font-family: \"Montserrat\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  background-color: rgb(21, 21, 37);\r\n  border: 0;\r\n  border-radius: 10px;\r\n  padding: 10px 30px;\r\n  color: white;\r\n}\r\n\r\n.feed {\r\n  padding-top: 70px;\r\n}\r\n\r\n.post {\r\n  background-color: rgb(26, 26, 46);\r\n  width: 630px;\r\n  height: 130px;\r\n  border-radius: 10px;\r\n  padding: 20px 20px;\r\n}\r\n\r\n.posts__list {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-direction: column;\r\n  gap: 30px;\r\n}\r\n\r\n.post__details {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.post__content {\r\n  margin-bottom: 40px;\r\n  font-family: \"Montserrat\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  color: rgb(240, 242, 255);\r\n}\r\n\r\n.post__author {\r\n  font-family: \"Roboto Condensed\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  font-size: 18px;\r\n  color: rgb(182, 194, 255);\r\n}\r\n\r\n.post__date {\r\n  font-family: \"Roboto Condensed\", serif;\r\n  font-optical-sizing: auto;\r\n  font-weight: 400;\r\n  font-style: normal;\r\n  font-size: 12px;\r\n  color: rgb(182, 194, 255);\r\n}\r\n\r\n.delete__btn {\r\n  width: 30px;\r\n  height: 30px;\r\n  background-color: transparent;\r\n  border: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n.like__btn {\r\n  width: 30px;\r\n  height: 30px;\r\n  background-color: transparent;\r\n  border: 0;\r\n  cursor: pointer;\r\n}\r\n\r\n.post__interactions {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.like-btn {\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 5px;\r\n  padding: 5px;\r\n  color: #657786;\r\n  transition: color 0.3s;\r\n}\r\n\r\n.like-btn:hover {\r\n  color: #e0245e;\r\n}\r\n\r\n.like-btn.liked {\r\n  color: #e0245e;\r\n}\r\n\r\n.like-svg {\r\n  width: 20px;\r\n  height: 20px;\r\n  fill: currentColor;\r\n}\r\n\r\n.like-count {\r\n  font-size: 10px;\r\n}"},"lineCount":248}},"error":null,"hash":"d39d2696f1477a3e3c9b64a69327467e","cacheData":{"env":{}}}